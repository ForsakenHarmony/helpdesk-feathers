{
  "name": "helpdesk-feathers",
  "description": "",
  "version": "0.1.1",
  "homepage": "",
  "main": "src/server/",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {},
  "author": "ForsakenHarmony",
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "test": "npm run xo && npm run eslint && npm run mocha",
    "ava": "ava",
    "xo": "xo",
    "eslint": "eslint src",
    "start": "node build/backend.js",
    "nodemon": "nodemon build/backend.js --watch build/",
    "nodemon:debug": "nodemon --inspect --debug-brk build/backend.js --watch build/",
    "mocha": "mocha test/ --recursive",
    "webpack:dev": "webpack --progress --colors --config webpack/webpack.config.dev.js",
    "webpack:prod": "webpack --progress --colors --config webpack/webpack.config.prod.js",
    "webpack:watch": "webpack --progress --colors --config webpack/webpack.config.dev.js --watch",
    "webpack:server": "webpack --progress --colors --config webpack/webpack.config.server.js",
    "webpack:server:watch": "webpack --progress --colors --config webpack/webpack.config.server.js --watch"
  },
  "xo": {
    "space": true,
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "import/prefer-default-export": "off",
      "no-multi-spaces": "off",
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": true
        }
      ],
      "key-spacing": "off",
      "no-empty-pattern": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "func-names": [
        "error",
        "as-needed"
      ],
      "arrow-parens": [
        "error",
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "react/react-in-jsx-scope": "off",
      "react/no-unknown-property": [
        "error",
        {
          "ignore": [
            "class"
          ]
        }
      ],
      "react/prop-types": "off",
      "react/jsx-uses-vars": "error"
    },
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "no-undef": 0
        }
      }
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bulma": "^0.3.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "debug": "^2.5.1",
    "feathers": "^2.0.3",
    "feathers-authentication": "^1.0.2",
    "feathers-authentication-client": "^0.1.7",
    "feathers-authentication-jwt": "^0.3.0",
    "feathers-authentication-local": "^0.3.3",
    "feathers-authentication-management": "^0.1.2",
    "feathers-authentication-oauth1": "^0.2.3",
    "feathers-authentication-oauth2": "^0.2.3",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.5.0",
    "feathers-hooks": "^1.7.1",
    "feathers-hooks-common": "^2.0.3",
    "feathers-permissions": "^0.1.1",
    "feathers-rest": "^1.5.2",
    "feathers-rethinkdb": "^0.3.3",
    "feathers-socketio": "^1.4.2",
    "font-awesome": "^4.7.0",
    "history": "^4.5.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-google-token": "^0.1.2",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.0",
    "preact-redux": "^2.0.0",
    "preact-render-to-string": "^3.3.0",
    "preact-router": "^2.4.0",
    "preact-router-redux": "^4.1.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "rethinkdbdash": "^2.3.27",
    "serve-favicon": "^2.3.2",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "source-map-support": "^0.4.11",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "buble": "^0.15.2",
    "buble-loader": "^0.4.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.4.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-node-externals": "^1.5.4",
    "xo": "^0.17.1"
  }
}
